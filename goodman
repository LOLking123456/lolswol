local ExampleSlider = Groupbox1:CreateSlider("Handles Speed", 1, 4, 0, function(value)
   _G.Modifier = (value) -- Higher Number: Faster (2 is considered fast)
local Player = game.Players.LocalPlayer
local Character = Player.Character or Player.CharacterAdded:Wait()

for i,v in pairs(getgc(true)) do
    if type(v) == "table" and rawget(v, "baseSpeed") then
        v.baseSpeed = tonumber(_G.Modifier)
        v.Stamina = 0
        v.stamina = 0
    end
end
end)

