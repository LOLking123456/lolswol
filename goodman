local Groupbox6 = Tab2:CreateGroupbox("Settings", "Left")

local ExampleButton = Groupbox1:CreateButton("Aimbot (Q)", function()
    game.Players.LocalPlayer:GetMouse().KeyDown:Connect(function(KeyPressed)
 if KeyPressed == "q" then
local args = {
    [1] = true,
    [2] = "Idle",
    [3] = false
}

game:GetService("ReplicatedStorage").Remotes.server.shoot:FireServer(unpack(args))
wait(0.41)


local args = {
    [1] = false,
    [2] = false
}

game:GetService("ReplicatedStorage").Remotes.server.shoot:FireServer(unpack(args))
end
end)("Pressed")
end)

